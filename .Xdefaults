!! URxvt

URxvt.depth:		32
URxvt.background: 	[95]#3f3f3f

URxvt.font:		xft:DejaVuSansMono Nerd Font Mono:size=9, xft:monospace:size=9
URxvt.scrollBar:	false
!URxvt.letterSpace:	-1

!!! Extensions

URxvt.perl-ext-common:	default,matcher,resize-font,url-select,keyboard-select

!! Matcher

URxvt.url-launcher:	/usr/bin/xdg-open
Urxvt.matcher.button:	1

!! resize-font

URxvt.resize-font.smaller:	C-j
URxvt.resize-font.bigger:	C-k

!! url-select

URxvt.keysym.M-u:		perl:url-select:select_next
URxvt.url-select.launcher:	/usr/bin/xdg-open
URxvt.usl-select.underline:	true

!! keyboard-select

URxvt.keysym.M-Escape:	perl:keyboard-select:activate
